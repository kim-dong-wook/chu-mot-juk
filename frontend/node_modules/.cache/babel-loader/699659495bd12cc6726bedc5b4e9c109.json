B,MAAA,SAAS,EAAE,QAAQ,CAAC,YAAM;AACxB,YAAM,SAAQ,GAAI,KAAK,CAAC,OAAN,CAAc,oBAAd,CAAlB;AACA,YAAI,IAAG,GAAI,MAAM,CAAC,IAAP,CAAY,SAAZ,CAAX;AACA,YAAI,SAAQ,GAAI,EAAhB;;AACA,aAAK,IAAI,CAAA,GAAI,CAAb,EAAgB,CAAA,GAAI,IAAI,CAAC,MAAzB,EAAiC,EAAE,CAAnC,EAAsC;AACpC,cAAI,UAAS,GAAI,EAAjB;AACA,UAAA,UAAU,CAAC,IAAX,GAAkB,SAAS,CAAC,IAAI,CAAC,CAAD,CAAL,CAAT,CAAmB,IAArC;AACA,UAAA,UAAU,CAAC,KAAX,GAAmB,SAAS,CAAC,IAAI,CAAC,CAAD,CAAL,CAAT,CAAmB,IAAtC;AACA,UAAA,SAAS,CAAC,IAAV,CAAe,UAAf;AACF;;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,SAAZ;AACA,eAAO,SAAP;AACD,OAZkB,CAFE;AAerB,MAAA,WAAW,EAAE,QAAQ,CAAC;AAAA,eAAM,KAAK,CAAC,OAAN,CAAc,8BAAd,CAAN;AAAA,OAAD;AAfA,KAAD,CAAtB;;AAkBA,QAAI,KAAK,CAAC,WAAN,KAAsB,CAAC,CAA3B,EAA8B;AAC5B,MAAA,KAAK,CAAC,WAAN,GAAoB,CAApB;AACA,MAAA,KAAK,CAAC,MAAN,CAAa,yBAAb,EAAwC,CAAxC;AACF;;AAEA,QAAM,UAAS,GAAI,SAAb,UAAa,CAAU,KAAV,EAAiB;AAClC,MAAA,KAAK,CAAC,MAAN,CAAa,yBAAb,EAAwC,KAAxC;AACA,UAAM,SAAQ,GAAI,KAAK,CAAC,OAAN,CAAc,oBAAd,CAAlB;AACA,UAAI,IAAG,GAAI,MAAM,CAAC,IAAP,CAAY,SAAZ,CAAX;AACA,MAAA,MAAM,CAAC,IAAP,CAAY;AACV,QAAA,IAAI,EAAE,IAAI,CAAC,KAAD;AADA,OAAZ;AAGF,KAPA;;AASA,WAAO;AAAE,MAAA,KAAK,EAAL,KAAF;AAAS,MAAA,UAAS,EAAT;AAAT,KAAP;AACF;AA9Ca,CAAf","sourcesContent":["<template>\r\n  <el-row\r\n    class=\"main-sidebar\"\r\n    :gutter=\"10\"\r\n    :style=\"{ 'width': width }\">\r\n    <div class=\"hide-on-small\">\r\n      <el-menu\r\n        :default-active=\"String(state.activeIndex)\"\r\n        active-text-color=\"#ffd04b\"\r\n        class=\"el-menu-vertical-demo\"\r\n        @select=\"menuSelect\">\r\n        <el-menu-item v-for=\"(item, index) in state.menuItems\" :key=\"index\" :index=\"index.toString()\">\r\n          <i v-if=\"item.icon\" :class=\"['ic', item.icon]\"/>\r\n          <span>{{ item.title }}</span>\r\n        </el-menu-item>\r\n      </el-menu>\r\n    </div>\r\n  </el-row>\r\n</template>\r\n<style>\r\n.main-sidebar .el-menu {\r\n  margin-top: 0;\r\n  padding-left: 0;\r\n}\r\n.main-sidebar .hide-on-small {\r\n  height: 100%;\r\n}\r\n.main-sidebar .hide-on-small .el-menu {\r\n  height: 100%;\r\n}\r\n.main-sidebar .el-menu .el-menu-item {\r\n  cursor: pointer;\r\n  border-right: none;\r\n}\r\n.main-sidebar .el-menu .el-menu-item .ic {\r\n  margin-right: 5px;\r\n}\r\n</style>\r\n<script>\r\nimport { reactive, computed } from 'vue'\r\nimport { useStore } from 'vuex'\r\nimport { useRouter } from 'vue-router'\r\n\r\nexport default {\r\n  name: 'main-header',\r\n\r\n  props: {\r\n    width: {\r\n      type: String,\r\n      default: '240px'\r\n    }\r\n  },\r\n  setup() {\r\n    const store = useStore()\r\n    const router = useRouter()\r\n\r\n    const state = reactive({\r\n      searchValue: null,\r\n      menuItems: computed(() => {\r\n        const MenuItems = store.getters['menuStore/getMenus']\r\n        let keys = Object.keys(MenuItems)\r\n        let menuArray = []\r\n        for (let i = 0; i < keys.length; ++i) {\r\n          let menuObject = {}\r\n          menuObject.icon = MenuItems[keys[i]].icon\r\n          menuObject.title = MenuItems[keys[i]].name\r\n          menuArray.push(menuObject)\r\n        }\r\n        console.log(menuArray)\r\n        return menuArray\r\n      }),\r\n      activeIndex: computed(() => store.getters['menuStore/getActiveMenuIndex'])\r\n    })\r\n\r\n    if (state.activeIndex === -1) {\r\n      state.activeIndex = 0\r\n      store.commit('menuStore/setMenuActive', 0)\r\n    }\r\n\r\n    const menuSelect = function (param) {\r\n      store.commit('menuStore/setMenuActive', param)\r\n      const MenuItems = store.getters['menuStore/getMenus']\r\n      let keys = Object.keys(MenuItems)\r\n      router.push({\r\n        name: keys[param]\r\n      })\r\n    }\r\n\r\n    return { state, menuSelect }\r\n  }\r\n}\r\n</script>\r\n"],"sourceRoot":""}]}